# TrendingVenues Podfile

source 'https://github.com/CocoaPods/Specs.git'

platform :ios, '8.0'

target :TrendingVenues do
    
    pod 'Mantle', 		 '1.3.1',    :inhibit_warnings => true
    pod 'Masonry',  	 '0.6.1',    :inhibit_warnings => true
    pod 'SDWebImage', 	 '3.7.1',    :inhibit_warnings => true

    target :TrendingVenuesTests, :exclusive => true do
        pod 'OHHTTPStubs', '3.1.7'
    end
end
